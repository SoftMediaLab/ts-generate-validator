{
  "name": "ts-generate-validator",
  "version": "0.0.1",
  "description": "Task to generate validation functions to check whether JSON data conforms to a schema described as typescript classes",
  "main": "src/index.ts",
  "bin": "dist/src/bin.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "fix": "npm run lint -- --fix",
    "test": "jest",
    "build": "tsc --resolveJsonModule --outDir dist src/bin.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SoftMediaLab/ts-generate-validator.git"
  },
  "keywords": [
    "codegeneration",
    "validation",
    "typescript"
  ],
  "author": "SoftMediaLab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SoftMediaLab/ts-generate-validator/issues"
  },
  "homepage": "https://github.com/SoftMediaLab/ts-generate-validator#readme",
  "devDependencies": {
    "@types/mkdirp": "1.0.1",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.30",
    "@types/nunjucks": "3.1.4",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "eslint": "7.20.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.3.1",
    "husky": "5.1.1",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "ts-jest": "26.5.2",
    "ts-node": "9.1.1",
    "typescript": "4.2.2"
  },
  "dependencies": {
    "prettier": "2.2.1",
    "mkdirp": "1.0.4",
    "nunjucks": "3.2.3",
    "path": "0.12.7",
    "ts-file-parser": "0.0.18"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint . --ext .ts --fix"
    ],
    "*.{js,json,md,ts}": [
      "prettier --write"
    ]
  }
}
